{
  "build": {
    "env": {
      "MONGODB_DB": "ducsu2025"
    }
  },
  "functions": {
    "app/api/**/*": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
